datacheck = "select distinct mast_schoollist.sch_num from mast_schoollist inner join mast_school_type using (sch_num, sch_year) inner join class1_5_enroll_app using (sch_num, sch_year) where mast_schoollist.flash=2 and mast_school_type.flash=2"

