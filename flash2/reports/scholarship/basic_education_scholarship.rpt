datacheck = "select distinct mast_schoollist.sch_num from mast_schoollist left join mast_school_type using(sch_num, sch_year) left join pr_scholarship using(sch_num, sch_year) left join lss_scholarship using(sch_num, sch_year) where mast_schoollist.flash2=1 and mast_schoollist.flash1=1 and (mast_school_type.class1>0)"

query1 = "select mast_schoollist.wardno from mast_schoollist left join mast_school_type using (sch_num, sch_year) left join pr_scholarship using (sch_num, sch_year) left join lss_scholarship using (sch_num, sch_year) where mast_schoollist.flash2=1 and mast_schoollist.flash1=1"

query2 = "select mast_schoollist.location from mast_schoollist left join mast_school_type using (sch_num, sch_year) left join pr_scholarship using (sch_num, sch_year) left join lss_scholarship using (sch_num, sch_year) where mast_schoollist.flash2=1 and mast_schoollist.flash1=1"

query3 = "select mast_schoollist.account_no from mast_schoollist left join mast_school_type using (sch_num, sch_year) left join pr_scholarship using (sch_num, sch_year) left join lss_scholarship using (sch_num, sch_year) where mast_schoollist.flash2=1 and mast_schoollist.flash1=1"

query4 = "select sum(female), sum(male), sum(total) from mast_schoollist left join mast_school_type using(sch_num, sch_year) left join pr_scholarship using(sch_num, sch_year) where mast_schoollist.flash2=1 and mast_schoollist.flash1=1 and pr_scholarship.class=1"

