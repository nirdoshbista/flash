; Report & Chart data for School by grade - Extra Information
;

[header]
; you can write as many titles as title1, title2, ... 
title1 = "Community Based ECD Information"

[prereq]
prereq1title = "School Type"
prereq1options = "All | Government Aided | Community Managed | Quota Teacher | Government Unaided | Government Supported | Institutional (All) | Institutional but Private Trust | Institutional but Public Trust | Institutional but Company | Madrassas | Gumbas | Ashrams | Community"
prereq1clauses = "mast_school_type.ecd>0| mast_school_type.ecd=1
| mast_school_type.ecd=2 | mast_school_type.ecd=3 | mast_school_type.ecd=4 | (mast_school_type.ecd=1 or mast_school_type.ecd=2 or mast_school_type.ecd=3 or mast_school_type.ecd=4) | (mast_school_type.ecd=5 or mast_school_type.ecd=6 or mast_school_type.ecd=7) | mast_school_type.ecd=5 | mast_school_type.ecd=6 | mast_school_type.ecd=7 | mast_school_type.ecd=8 | mast_school_type.ecd=9 | mast_school_type.ecd=10 | mast_school_type.ecd=11 | mast_school_type.ecd=12" 

prereq2title = "Year"
prereq2options = "2065|2064|2063|2062"
prereq2clauses = "mast_schoollist.sch_year=2065 and mast_school_type.sch_year=2065|mast_schoollist.sch_year=2064 and mast_school_type.sch_year=2064|mast_schoollist.sch_year=2063 and mast_school_type.sch_year=2063|mast_schoollist.sch_year=2062 and mast_school_type.sch_year=2062"

[tableheader]
row1 = "Code / Name [2]| Total ECDs | Ward | Location | Telephone | Email "

[row]
datacheck = "select distinct (mast_schoollist.sch_num) from mast_schoollist left join mast_school_type using (sch_num, sch_year) left join ecdppc_enroll using (sch_num, sch_year) left join ecdppc_info using (sch_num, sch_year) where mast_schoollist.flash=2 and mast_school_type.flash=2 and ecdppc_enroll.ecd_num>1"

query1 = "select count(mast_schoollist.sch_num) from mast_schoollist left join mast_school_type using (sch_num, sch_year) left join ecdppc_enroll using (sch_num, sch_year) where mast_schoollist.flash=2 and mast_school_type.flash=2 and ecd>0 and ecdppc_enroll.ecd_num>1"

query2 = "select ecdppc_info.ecd_tole from mast_schoollist left join mast_school_type using (sch_num, sch_year) left join ecdppc_enroll using (sch_num, sch_year) where mast_schoollist.flash=2 and mast_school_type.flash=2 and ecdppc_enroll.ecd_num>1"
