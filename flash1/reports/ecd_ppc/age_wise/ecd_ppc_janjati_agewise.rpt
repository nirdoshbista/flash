[tableheader]
row1 = "Code / Name [3,2]  | Year 2067 Onwards [15] | Upto 2066 [12]"
row2 = "3 [1,3] | 4 [1,3] | 5 [1,3] | >5 [1,3] | Total [1,3] | < 3 [1,3]| 3 - 4 [1,3]| > 4 [1,3] | Total [1,3]"
row3 = "G | B | T | G | B | T | G | B | T | G | B | T | G | B | T | G | B | T | G | B | T | G | B | T | G | B | T "

[row]
datacheck = "select distinct(mast_schoollist.sch_num) from mast_schoollist left join mast_school_type using (sch_num, sch_year) left join ecd_janjati_enroll_age_f1 using (sch_num, sch_year) where mast_schoollist.flash1=1"

query1 = "select sum(f3), sum(m3), sum(f3)+sum(m3), sum(f4), sum(m4), sum(f4)+sum(m4), sum(f5), sum(m5), sum(f5)+sum(m5), sum(f_g5), sum(m_g5), sum(f_g5)+sum(m_g5), sum(f3)+sum(f4)+sum(f5)+sum(f_g5), sum(m3)+sum(m4)+sum(m5)+sum(m_g5), sum(f3)+sum(m3)+sum(f4)+sum(m4)+sum(f5)+sum(m5)+sum(f_g5)+sum(m_g5) from mast_schoollist left join mast_school_type using (sch_num, sch_year) left join ecd_janjati_enroll_age_f1 using (sch_num, sch_year) where mast_schoollist.flash1=1"

query2 = "select sum(f_l3), sum(m_l3), sum(f_l3)+sum(m_l3), sum(f_3_4), sum(m_3_4), sum(f_3_4)+sum(m_3_4), sum(f_g4), sum(m_g4), sum(f_g4)+sum(m_g4), sum(f_l3)+sum(f_3_4)+sum(f_g4), sum(m_l3)+sum(m_3_4)+sum(m_g4), sum(f_l3)+sum(m_l3)+sum(f_3_4)+sum(m_3_4)+sum(f_g4)+sum(m_g4) from mast_schoollist left join mast_school_type using (sch_num, sch_year) left join ecd_janjati_enroll_age_f1 using (sch_num, sch_year) where mast_schoollist.flash1=1"

