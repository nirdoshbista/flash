datacheck = "select mast_schoollist.sch_num from mast_schoollist left join mast_school_type using(sch_num, sch_year) left join  hsec_last_exam_details_f1  using (sch_num, sch_year) where mast_schoollist.flash1=1"

query1 = "select mast_schoollist.wardno from mast_schoollist left join mast_school_type using (sch_num, sch_year) left join hsec_last_exam_details_f1 using (sch_num, sch_year) where mast_schoollist.flash1=1"

query2 = "select mast_schoollist.location from mast_schoollist left join mast_school_type using (sch_num, sch_year) left join hsec_last_exam_details_f1 using (sch_num, sch_year) where mast_schoollist.flash1=1"

query3 = "select mast_schoollist.account_no from mast_schoollist left join mast_school_type using (sch_num, sch_year) left join hsec_last_exam_details_f1 using (sch_num, sch_year) where mast_schoollist.flash1=1"

query4 = "select sum(tot_app_f), sum(tot_app_m), sum(tot_app_t), sum(dalit_app_f), sum(dalit_app_m), sum(dalit_app_t), sum(janjati_app_f), sum(janjati_app_m), sum(janjati_app_t), sum(tot_pass_f), sum(tot_pass_m), sum(tot_pass_t), sum(dalit_pass_f), sum(dalit_pass_m), sum(dalit_pass_t), sum(janjati_pass_f), sum(janjati_pass_m), sum(janjati_pass_t) from mast_schoollist left join mast_school_type using(sch_num, sch_year) left join  hsec_last_exam_details_f1 using (sch_num, sch_year) where mast_schoollist.flash1=1 and hsec_last_exam_details_f1.class=11"

query5 = "select sum(tot_app_f), sum(tot_app_m), sum(tot_app_t), sum(dalit_app_f), sum(dalit_app_m), sum(dalit_app_t), sum(janjati_app_f), sum(janjati_app_m), sum(janjati_app_t), sum(tot_pass_f), sum(tot_pass_m), sum(tot_pass_t), sum(dalit_pass_f), sum(dalit_pass_m), sum(dalit_pass_t), sum(janjati_pass_f), sum(janjati_pass_m), sum(janjati_pass_t) from mast_schoollist left join mast_school_type using(sch_num, sch_year) left join  hsec_last_exam_details_f1 using (sch_num, sch_year) where mast_schoollist.flash1=1 and hsec_last_exam_details_f1.class=12"

