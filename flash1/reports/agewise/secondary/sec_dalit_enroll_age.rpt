[row]
datacheck = "select distinct mast_schoollist.sch_num from mast_schoollist left join mast_school_type using (sch_num, sch_year) left join sec_dalit_enroll_age_f1 using (sch_num, sch_year) where mast_schoollist.flash1=1 and mast_school_type.class9>0"

query1 = "select sum(f_l13), sum(m_l13), sum(t_l13), sum(f_13), sum(m_13), sum(t_13), sum(f_13_14), sum(m_13_14), sum(t_13_14), sum(f_g14), sum(m_g14), sum(t_g14) from mast_schoollist left join mast_school_type using (sch_num, sch_year) left join sec_dalit_enroll_age_f1 using (sch_num, sch_year) where mast_schoollist.flash1=1 and sec_dalit_enroll_age_f1.class=9"

query2 = "select  sum(f_l14), sum(m_l14), sum(t_l14), sum(f_14), sum(m_14), sum(t_14), sum(f_g14), sum(m_g14), sum(t_g14) from mast_schoollist left join mast_school_type using (sch_num, sch_year) left join sec_dalit_enroll_age_f1 using (sch_num, sch_year) where mast_schoollist.flash1=1 and sec_dalit_enroll_age_f1.class=10"



