[row]
datacheck = "select distinct mast_schoollist.sch_num from mast_schoollist left join mast_school_type using (sch_num, sch_year) left join pr_total_enroll_age_f1 using (sch_num, sch_year) where mast_schoollist.flash1=1 and mast_school_type.class1>0"

query1 = "select sum(f_l5), sum(m_l5), sum(t_l5 ),sum(f_5), sum(m_5), sum(t_5),sum(f_6), sum(m_6), sum(t_6),  sum(f_7_9), sum(m_7_9), sum(t_7_9),  sum(f_g9), sum(m_g9), sum(t_g9) from mast_schoollist left join mast_school_type using (sch_num, sch_year) left join pr_total_enroll_age_f1 using (sch_num, sch_year) where mast_schoollist.flash1=1 and pr_total_enroll_age_f1.class=1"

query2 = "select sum(f_l6), sum(m_l6), sum(t_l6),sum(f_6), sum(m_6), sum(t_6) ,sum(f_7_9), sum(m_7_9), sum(t_7_9),  sum(f_g9), sum(m_g9), sum(t_g9) from mast_schoollist left join mast_school_type using (sch_num, sch_year) left join pr_total_enroll_age_f1 using (sch_num, sch_year) where mast_schoollist.flash1=1 and pr_total_enroll_age_f1.class=2"

query3 = "select sum(f_l7), sum(m_l7), sum(t_l7), sum(f_7), sum(m_7), sum(t_7) ,sum(f_8_9), sum(m_8_9), sum(t_8_9),  sum(f_g9), sum(m_g9), sum(t_g9) from mast_schoollist left join mast_school_type using (sch_num, sch_year) left join pr_total_enroll_age_f1 using (sch_num, sch_year) where mast_schoollist.flash1=1 and pr_total_enroll_age_f1.class=3"

query4 = "select sum(f_l8), sum(m_l8), sum(t_l8), sum(f_8), sum(m_8), sum(t_8) ,sum(f_9), sum(m_9), sum(t_9),  sum(f_g9), sum(m_g9), sum(t_g9) from mast_schoollist left join mast_school_type using (sch_num, sch_year) left join pr_total_enroll_age_f1 using (sch_num, sch_year) where mast_schoollist.flash1=1 and pr_total_enroll_age_f1.class=4"

query5 = "select sum(f_l9), sum(m_l9), sum(t_l9), sum(f_9), sum(m_9), sum(t_9), sum(f_g9), sum(m_g9), sum(t_g9) from mast_schoollist left join mast_school_type using (sch_num, sch_year) left join pr_total_enroll_age_f1 using (sch_num, sch_year) where mast_schoollist.flash1=1 and pr_total_enroll_age_f1.class=5"
