[row]
datacheck = "select distinct mast_schoollist.sch_num from mast_schoollist left join mast_school_type using (sch_num, sch_year) left join hsec_total_enroll_age_f1 using (sch_num, sch_year) where mast_schoollist.flash1=1 and mast_school_type.class11>0"

query1 = "select sum(f_l15), sum(m_l15), sum(t_l15), sum(f_15), sum(m_15), sum(t_15), sum(f_15_16), sum(m_15_16), sum(t_15_16), sum(f_g16), sum(m_g16), sum(t_g16) from mast_schoollist left join mast_school_type using (sch_num, sch_year) left join hsec_total_enroll_age_f1 using (sch_num, sch_year) where mast_schoollist.flash1=1 and hsec_total_enroll_age_f1.class=11"

query2 = "select  sum(f_l16), sum(m_l16), sum(t_l16), sum(f_16), sum(m_16), sum(t_16), sum(f_g16), sum(m_g16), sum(t_g16) from mast_schoollist left join mast_school_type using (sch_num, sch_year) left join hsec_total_enroll_age_f1 using (sch_num, sch_year) where mast_schoollist.flash1=1 and hsec_total_enroll_age_f1.class=12"