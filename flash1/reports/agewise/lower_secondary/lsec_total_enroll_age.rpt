row3 = " G | B | T|G | B | T | G | B | T |G | B | T | G | B | T | G | B | T | G | B | T| G | B | T| G | B | T | G | B | T| G | B | T"


[row]
datacheck = "select distinct mast_schoollist.sch_num from mast_schoollist left join mast_school_type using (sch_num, sch_year) left join sec_total_enroll_age_f1 using (sch_num, sch_year) where mast_schoollist.flash1=1 and mast_school_type.class6>0"

query1 = "select sum(f_l10), sum(m_l10), sum(t_l10 ),sum(f_10), sum(m_10), sum(t_10),sum(f_11_12), sum(m_11_12), sum(t_11_12),  sum(f_g12), sum(m_g12), sum(t_g12) from mast_schoollist left join mast_school_type using (sch_num, sch_year) left join sec_total_enroll_age_f1 using (sch_num, sch_year) where mast_schoollist.flash1=1 and sec_total_enroll_age_f1.class=6"

query2 = "select sum(f_l11),sum(m_l11), sum(t_l11), sum(f_11), sum(m_11), sum(t_11), sum(f_12), sum(m_12), sum(t_12), sum(f_g12), sum(m_g12), sum(t_g12) from mast_schoollist left join mast_school_type using (sch_num, sch_year) left join sec_total_enroll_age_f1 using (sch_num, sch_year) where mast_schoollist.flash1=1 and sec_total_enroll_age_f1.class=7"

query3 = "select sum(f_l12), sum(m_l12), sum(t_l12), sum(f_12), sum(m_12), sum(t_12), sum(f_g12), sum(m_g12), sum(t_g12) from mast_schoollist left join mast_school_type using (sch_num, sch_year) left join sec_total_enroll_age_f1 using (sch_num, sch_year) where mast_schoollist.flash1=1 and sec_total_enroll_age_f1.class=8"

