[row]
datacheck = "select mast_schoollist.sch_num from mast_schoollist left join mast_school_type using (sch_num, sch_year) left join last_class1_5_enroll_f1 using (sch_num, sch_year) where mast_schoollist.flash1=1"

query1 = "select mast_schoollist.wardno from mast_schoollist left join mast_school_type using (sch_num, sch_year) left join last_class1_5_enroll_f1 using (sch_num, sch_year) where mast_schoollist.flash1=1"

query2 = "select mast_schoollist.location from mast_schoollist left join mast_school_type using (sch_num, sch_year) left join last_class1_5_enroll_f1 using (sch_num, sch_year) where mast_schoollist.flash1=1"

query3 = "select mast_schoollist.account_no from mast_schoollist left join mast_school_type using (sch_num, sch_year) left join last_class1_5_enroll_f1 using (sch_num, sch_year) where mast_schoollist.flash1=1"

query4 = "select sum(tot_enroll_janjati_f), sum(tot_enroll_janjati_m), sum(tot_enroll_janjati_t), sum(tot_appeared_exam_janjati_f), sum(tot_appeared_exam_janjati_m), sum(tot_appeared_exam_janjati_t), sum(tot_passed_exam_janjati_f), sum(tot_passed_exam_janjati_m), sum(tot_passed_exam_janjati_t) from mast_schoollist left join mast_school_type using (sch_num, sch_year) left join last_class1_5_enroll_f1 using (sch_num, sch_year) where mast_schoollist.flash1=1"

