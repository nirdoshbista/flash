[row]
datacheck = "select distinct mast_schoollist.sch_num from mast_schoollist left join mast_school_type using (sch_num, sch_year) left join sec_total_enroll_age_f1 using (sch_num, sch_year) where mast_schoollist.flash=1 and mast_school_type.flash=1 and mast_school_type.class6>0"
query1 = "SELECT sum(f_l10),sum(m_l10),sum(t_l10),sum(f_10),sum(m_10),sum(t_10),sum(f_11),sum(m_11),sum(t_11),sum(f_12),sum(m_12),sum(t_12),sum(f_13),sum(m_13),sum(t_13),sum(f_14),sum(m_14),sum(t_14),sum(f_g14),sum(m_g14),sum(t_g14)  from mast_schoollist inner join mast_school_type using (sch_num, sch_year) inner join sec_total_enroll_age_f1 using (sch_num, sch_year) where mast_schoollist.flash=1 and sec_total_enroll_age_f1.class=6"
