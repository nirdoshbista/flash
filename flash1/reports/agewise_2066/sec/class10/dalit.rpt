datacheck = "select distinct mast_schoollist.sch_num from mast_schoollist left join mast_school_type using (sch_num, sch_year) left join sec_dalit_enroll_age_f1 using (sch_num, sch_year) where mast_schoollist.flash=1 and mast_school_type.flash=1 and mast_school_type.class9>0"
query1 = "SELECT sum(f_l14),sum(m_l14),sum(t_l14),sum(f_14),sum(m_14),sum(t_14),sum(f_15),sum(m_15),sum(t_15),sum(f_16),sum(m_16),sum(t_16),sum(f_17),sum(m_17),sum(t_17),sum(f_g17),sum(m_g17),sum(t_g17)  from mast_schoollist inner join mast_school_type using (sch_num, sch_year) inner join sec_dalit_enroll_age_f1 using (sch_num, sch_year) where mast_schoollist.flash=1 and sec_dalit_enroll_age_f1.class=10"
