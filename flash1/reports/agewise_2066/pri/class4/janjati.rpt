query1 = "SELECT sum(f_l8),sum(m_l8),sum(t_l8),sum(f_8),sum(m_8),sum(t_8),sum(f_9),sum(m_9),sum(t_9),sum(f_10),sum(m_10),sum(t_10),sum(f_11),sum(m_11),sum(t_11),sum(f_g11),sum(m_g11),sum(t_g11)  from mast_schoollist inner join mast_school_type using (sch_num, sch_year) inner join pr_janjati_enroll_age_f1 using (sch_num, sch_year) where mast_schoollist.flash=1 and pr_janjati_enroll_age_f1.class=4"
