row2 = "Girls | Boys | Total | Girls | Boys | Total | Girls | Boys | Total | Girls | Boys | Total "

[row]
datacheck = "select distinct mast_schoollist.sch_num from mast_schoollist left join mast_school_type using (sch_num, sch_year) left join enr_rep_mig_class6_8_f1 using (sch_num, sch_year) where mast_schoollist.flash=1 and mast_school_type.flash=1"

query1 = "select sum(tot_tran_others_f), sum(tot_tran_others_m), sum(tot_tran_others_t) from mast_schoollist left join mast_school_type using (sch_num, sch_year) left join enr_rep_mig_class6_8_f1 using (sch_num, sch_year) where mast_schoollist.flash=1 and mast_school_type.flash=1 and enr_rep_mig_class6_8_f1.class=6"

query2 = "select sum(tot_tran_others_f), sum(tot_tran_others_m), sum(tot_tran_others_t) from mast_schoollist left join mast_school_type using (sch_num, sch_year) left join enr_rep_mig_class6_8_f1 using (sch_num, sch_year) where mast_schoollist.flash=1 and mast_school_type.flash=1 and enr_rep_mig_class6_8_f1.class=7"

query3 = "select sum(tot_tran_others_f), sum(tot_tran_others_m), sum(tot_tran_others_t) from mast_schoollist left join mast_school_type using (sch_num, sch_year) left join enr_rep_mig_class6_8_f1 using (sch_num, sch_year) where mast_schoollist.flash=1 and mast_school_type.flash=1 and enr_rep_mig_class6_8_f1.class=8"

query4 = "select sum(tot_tran_others_f), sum(tot_tran_others_m), sum(tot_tran_others_t) from mast_schoollist left join mast_school_type using (sch_num, sch_year) left join enr_rep_mig_class6_8_f1 using (sch_num, sch_year) where mast_schoollist.flash=1 and mast_school_type.flash=1"
