; Report & Chart data for School for type
;

[property]
schoolwise = 0


[header]
; you can write as many titles as title1, title2, ... 
title1 = "Last Year Secondary Level Janjati Enrollment, Appeared and Passed in Exam by class"

[prereq]
prereq1title = "Class wise"
prereq1options = " Class 9| Class 10| Class9-10"
prereq1clauses = "last_class9_10_enroll_f1.class=9|last_class9_10_enroll_f1.class=10|(last_class9_10_enroll_f1.class=9 or last_class9_10_enroll_f1.class=10)"

prereq2title = "School Type"
prereq2options = "All | Government Aided | Community Managed | Quota Teachers | Government Unaided | Government Supported | Institutional (All) | Institutional but Private Trust | Institutional but Public Trust | Institutional but Company | Madrassas | Gumbas | Ashrams| SOP"
prereq2clauses ="mast_school_type.class9<11|mast_school_type.class9=1|mast_school_type.class9=2|mast_school_type.class9=3 | mast_school_type.class9=4 | (mast_school_type.class9=1 or mast_school_type.class9=2 or mast_school_type.class9=3 or mast_school_type.class9=4) | (mast_school_type.class9=5 or mast_school_type.class9=6 or mast_school_type.class9=7) | mast_school_type.class9=5 | mast_school_type.class9=6 | mast_school_type.class9=7 | mast_school_type.class9=8 | mast_school_type.class9=9 | mast_school_type.class9=10 | mast_school_type.class9=11"






prereq3title = "Year"
prereq3options = "2066|2065|2064|2063"
prereq3clauses = "mast_schoollist.sch_year=2066|mast_schoollist.sch_year=2065|mast_schoollist.sch_year=2064|mast_schoollist.sch_year=2063"

[tableheader]
row1 = "Code / Name [3,2] |Total[9] "
row2 = "Total Enrollment[3] | Appeared in Exam[3] |passed in exam[3]"
row3 = "G|B|T | G|B|T | G|B|T "

[row]
datacheck = "select mast_schoollist.sch_num from mast_schoollist inner join mast_school_type using (sch_num, sch_year) inner join last_class9_10_enroll_f1 using (sch_num, sch_year) where mast_schoollist.flash=1"
query1 = "select sum(tot_enroll_janjati_f), sum(tot_enroll_janjati_m), sum(tot_enroll_janjati_t), sum(tot_appeared_exam_janjati_f), sum(tot_appeared_exam_janjati_m), sum(tot_appeared_exam_janjati_t), sum(tot_passed_exam_janjati_f), sum(tot_passed_exam_janjati_m), sum(tot_passed_exam_janjati_t) from mast_schoollist inner join mast_school_type using (sch_num, sch_year) inner join last_class9_10_enroll_f1 using (sch_num, sch_year) where mast_schoollist.flash=1"

