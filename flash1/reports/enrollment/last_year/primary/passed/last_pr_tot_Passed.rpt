query5 = "select sum(tot_passed_exam_total_f), sum(tot_passed_exam_total_m), sum(tot_passed_exam_total_t) from mast_schoollist inner join mast_school_type using (sch_num, sch_year) inner join last_class1_5_enroll_f1 using (sch_num, sch_year) where mast_schoollist.flash=1 and class=5"

